{
  "name": "cryptopi-crew-metadata",
  "description": "CLI for generating Cryptopi Crew metadata and publishing to IPFS",
  "version": "0.0.0",
  "author": "David Purkiss",
  "bin": {
    "cryptopi-meta": "./bin/run"
  },
  "bugs": "https://github.com/CryptopiCrew/cryptopi-crew-metadata/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "chance": "^1.1.8",
    "progress": "^2.0.3",
    "signale": "^1.4.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/chance": "^1.1.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/progress": "^2.0.5",
    "@types/signale": "^1.4.2",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/CryptopiCrew/cryptopi-crew-metadata",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cryptopi-meta",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "CryptopiCrew/cryptopi-crew-metadata",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
